{
  "scripts": {
    "css": "node-sass --output-style compressed sass/style.scss -o .",
    "postcss": "postcss style.css --no-map --use autoprefixer -o style.css --autoprefixer.browsers=\"last 2 versions\"",
    "editor": "node-sass sass/style-editor.scss style-editor.css",
    "watch": "node-sass --output-style compressed -w sass -r -o .",
    "sync": "browser-sync start --proxy \"localhost/vietstart\" --files \"style.css\""
  },
  "dependencies": {
    "node-sass": "^4.14.1"
  }
}